        "ANDROID_FIREBASE_INSTALLATIONS_VERSION": "17.1.0",
        "ANDROID_FIREBASE_CRASHLYTICS_VERSION": "18.3.2",
        "ANDROID_FIREBASE_CRASHLYTICS_NDK_VERSION": "18.3.2",
